// function that puts basic student data into a table
int main() {

    cout << "   Course \t | \t Student\t|" << endl;
    cout << "------------------------------" << endl;
    cout << "\t C++ \t |\t\t100 \t|" << endl;
    cout << "------------------------------" << endl;
    cout << " Javascript\t |\t\t50\t\t|" << endl;
    cout << "------------------------------" << endl;
}
